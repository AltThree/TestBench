{
    "name": "alt-three/testbench",
    "description": "Provides Some Testing Traits For Apps",
    "keywords": ["testbench", "app", "TestBench", "Alt Three"],
    "license": "MIT",
    "authors": [
        {
            "name": "James Brooks",
            "email": "james@alt-three.com"
        },
        {
            "name": "Graham Campbell",
            "email": "graham@alt-three.com"
        },
        {
            "name": "Joseph Cohen",
            "email": "joe@alt-three.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "graham-campbell/testbench-core": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "nikic/php-parser": "^2.0|^3.0"
    },
    "suggest": {
        "laravel/framework": "Enables the use of every trait.",
        "nikic/php-parser": "Enables the use of the static analyzer."
    },
    "autoload": {
        "psr-4": {
            "AltThree\\TestBench\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AltThree\\Tests\\TestBench\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
